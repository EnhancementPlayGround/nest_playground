{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-test.ts$",
  "moduleNameMapper": {
    "^@libs/(.*)$": "<rootDir>/src/libs/$1",
    "^@config": "<rootDir>/src/config",
    "^@middlewares": "<rootDir>/src/middlewares"
  },
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
